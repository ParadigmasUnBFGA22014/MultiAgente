CREATE SCHEMA `paradigmasJade` DEFAULT CHARACTER SET utf8 ;

CREATE TABLE `ARREMATANTE` (
  `ID` int(11) NOT NULL,
  `NOME` varchar(45) DEFAULT NULL,
  `VALOR_DINHEIRO` decimal(10,0) DEFAULT NULL,
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

  
  
  
 CREATE TABLE `LANCES` (
  `ID` int(11) NOT NULL,
  `ID_ARREMATANTE` int(11) DEFAULT NULL,
  `ID_OBJETO` int(11) DEFAULT NULL,
  `VALOR_LANCE` decimal(10,0) DEFAULT NULL,
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


  
  CREATE TABLE `paradigmasJade`.`OBJETO` (
  `ID` INT NOT NULL,
  `NOME` VARCHAR(45) NULL,
  `LANCE_INICIAL` DECIMAL(10) NULL,
  `VALOR_INCREMENTO` DECIMAL(10) NULL,
  PRIMARY KEY (`ID`));
  
  