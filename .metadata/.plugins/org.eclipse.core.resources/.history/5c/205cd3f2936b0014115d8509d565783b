CREATE SCHEMA `paradigmasJade` DEFAULT CHARACTER SET utf8 ;

CREATE TABLE `paradigmasJade`.`ARREMATANTE` (
  `ID` INT NOT NULL,
  `NOME` VARCHAR(45) NULL,
  `VALOR_DINHEIRO` VARCHAR(45) NULL,
  PRIMARY KEY (`ID`));
  
  
  
  CREATE TABLE `paradigmasJade`.`LANCES` (
  `ID` INT NOT NULL,
  `ID_ARREMATANTE` INT NULL,
  `ID_OBJETO` INT NULL,
  `VALOR_LANCE` VARCHAR(45) NULL,
  PRIMARY KEY (`ID`));
  
  
  